{
  "name": "ai-file-cleanup",
  "version": "1.0.0",
  "private": true,
  "author": "AI File Cleanup Team",
  "description": "AI-powered file deduplication system with web and desktop apps",
  "scripts": {
    "dev": "turbo run dev --parallel",
    "dev:log": "powershell -ExecutionPolicy Bypass -File dev-with-logging.ps1",
    "dev:all": "turbo run dev --parallel",
    "dev:frontend": "turbo run dev --filter=@ai-cleanup/web --filter=@ai-cleanup/desktop --parallel",
    "dev:backend": "turbo run dev --filter=@ai-cleanup/api --filter=@ai-cleanup/ml-service --parallel",
    "dev:web": "turbo run dev --filter=@ai-cleanup/web",
    "dev:desktop": "turbo run dev --filter=@ai-cleanup/desktop",
    "dev:packages": "turbo run dev --filter=./packages/*",
    "dev:api": "turbo run dev --filter=@ai-cleanup/api",
    "dev:ml": "turbo run dev --filter=@ai-cleanup/ml-service",
    "build": "turbo run build",
    "build:web": "turbo run build --filter=@ai-cleanup/web",
    "build:desktop": "turbo run build --filter=@ai-cleanup/desktop",
    "build:packages": "turbo run build --filter=./packages/*",
    "lint": "turbo run lint",
    "lint:fix": "turbo run lint -- --fix",
    "typecheck": "turbo run typecheck",
    "test": "turbo run test",
    "clean": "turbo run clean && rimraf node_modules/.cache",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md,yml,yaml}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md,yml,yaml}\"",
    "db:generate": "pnpm --filter=@ai-cleanup/db db:generate",
    "api:generate-prisma": "node scripts/generate-prisma-client.js",
    "db:push": "pnpm --filter=@ai-cleanup/db db:push",
    "db:migrate": "pnpm --filter=@ai-cleanup/db db:migrate",
    "db:migrate:deploy": "pnpm --filter=@ai-cleanup/db db:migrate:deploy",
    "db:studio": "pnpm --filter=@ai-cleanup/db db:studio",
    "db:seed": "pnpm --filter=@ai-cleanup/db db:seed",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:restart": "docker-compose restart",
    "setup": "pnpm install && pnpm build:packages && pnpm db:generate",
    "setup:full": "pnpm install && pnpm docker:up && pnpm db:generate && pnpm db:push && pnpm build:packages",
    "start:web": "turbo run start --filter=@ai-cleanup/web",
    "package:desktop": "pnpm --filter=@ai-cleanup/desktop package",
    "package:desktop:win": "pnpm --filter=@ai-cleanup/desktop package:win",
    "package:desktop:mac": "pnpm --filter=@ai-cleanup/desktop package:mac",
    "package:desktop:linux": "pnpm --filter=@ai-cleanup/desktop package:linux",
    "deploy:installers": "powershell -ExecutionPolicy Bypass -File copy-installers-to-web.ps1",
    "vercel:install": "pnpm install --ignore-scripts"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.1.0",
    "prettier": "^3.1.0",
    "rimraf": "^5.0.5",
    "typescript": "^5.3.2"
  },
  "packageManager": "pnpm@8.11.0",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "dependencies": {
    "turbo": "^2.5.8"
  }
}
