{
  "name": "@ai-cleanup/db",
  "version": "1.0.0",
  "private": true,
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:migrate:both": "node scripts/migrate-both-dbs.js",
    "db:studio": "prisma studio",
    "db:seed": "tsx src/seed.ts"
  },
  "dependencies": {
    "@ai-cleanup/types": "workspace:*",
    "@prisma/client": "^5.7.1",
    "pgvector": "^0.1.8"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "prisma": "^5.7.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.2"
  }
}
